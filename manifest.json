{
  "manifest_version": 2,
  "name": "La D\u00e9p\u00eache UNLOCK",
  "version": "2.1",
  "description": "Obtenir un accès gratuit aux articles payants du site d'actualité \"La Dépêche\"",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.ladepeche.fr/*",
        "https://ladepeche.fr/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "logos/logo16.png",
      "32": "logos/logo32.png",
      "48": "logos/logo48.png",
      "128": "logos/logo128.png"
    }
  },
  "icons": {
    "16": "logos/logo16.png",
    "32": "logos/logo32.png",
    "48": "logos/logo48.png",
    "128": "logos/logo128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{a043526a-a765-4fe9-8fd4-c2297873a8cd}"
    }
  }
}
